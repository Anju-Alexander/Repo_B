{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base"
  ],
  "packageRules": [
    {
      "enabled": false,
      "matchPackagePatterns": ["*"],
      "automerge": true,
      "platformAutomerge": true,
      "automergeType": "pr"
    }
  ],
 "vulnerabilityAlerts": {
    "enabled": true,
    "labels": ["security"],
    "automerge": true
  },
  "osvVulnerabilityAlerts": true,
  "ignoreTests": true
  
}
